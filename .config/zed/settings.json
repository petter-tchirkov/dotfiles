{
  // assistant
  "edit_predictions": {
    "mode": "subtle"
  },
  "features": {
    "edit_prediction_provider": "none"
  },
  "agent": {
    "default_model": {
      "provider": "google",
      "model": "gemini-2.0-flash"
    },
    "single_file_review": true,
    "model_parameters": [],
    "always_allow_tool_actions": true,
    "version": "2",
    "button": false,
    "commit_message_model": {
      "provider": "google",
      "model": "gemini-2,5-pro-preview-05-06"
    }
  },
  // vim stuff
  "vim_mode": true,
  "vim": {
    "use_smartcase_find": true,
    "enable_vim_sneak": true
  },
  "base_keymap": "VSCode",
  "relative_line_numbers": true,
  "cursor_blink": false,
  "buffer_line_height": {
    "custom": 2
  },
  "vertical_scroll_margin": 10,
  // zen mode
  "centered_layout": {
    "left_padding": 0.05,
    "right_padding": 0.05
  },
  // clean
  "scrollbar": {
    "show": "never"
  },
  "tab_bar": {
    "show": false
  },
  "gutter": {
    "folds": false,
    "breakpoints": false,
    "runnables": false
  },
  "toolbar": {
    "breadcrumbs": false,
    "quick_actions": false,
    "agent_review": false,
    "selections_menu": false
  },
  "indent_guides": {
    // "active_line_width": 2,
    // "inactive_line_width": 1
    "enabled": false
  },
  "file_finder": {
    "modal_max_width": "xlarge"
  },
  "project_panel": {
    "dock": "right",
    "indent_size": 30,
    "button": false
  },
  "notification_panel": {
    "button": false
  },
  "collaboration_panel": {
    "button": false
  },
  "outline_panel": {
    "button": false
  },
  "git_panel": {
    "button": false
  },
  "search": {
    "button": false
  },
  "show_call_status_icon": false,
  "git": {
    "git_gutter": "hide"
  },
  "active_pane_modifiers": {
    "border_size": 0
  },
  "hover_popover_delay": 100,
  "unstable.ui_density": "compact",
  "hard_tabs": true,
  "buffer_font_features": {
    "calt": false
  },
  // ui font
  "ui_font_size": 16,
  "ui_font_family": "SpaceMono Nerd Font",
  "ui_font_weight": 700,
  // buffer font
  "buffer_font_family": "SpaceMono Nerd Font",
  "buffer_font_size": 16,
  "buffer_font_weight": 500,
  // theme
  "theme": {
    "mode": "system",
    "light": "Kanagawa",
    "dark": "One Dark"
  },
  "experimental.theme_overrides": {
    "border": null,
    "border.variant": "#1f1f28",
    "editor.line_number": "#54546d",
    "editor.active_line_number": "#FF9E3B"
  },
  // terminal
  "terminal": {
    "font_family": "SpaceMono Nerd Font",
    "font_size": 16,
    "default_height": 9999,
    "line_height": {
      "custom": 1.5
    },
    "button": false
  },
  "language_models": {
    "google": {
      "available_models": [
        {
          "name": "gemini-2.5-pro-preview-05-06",
          "display_name": "Gemini 2.5 Pro Preview 05-06",
          "max_tokens": 1000000
        }
      ]
    }
  },
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  },
  "prettier": {
    "allowed": false
  },
  "languages": {
    "JSONC": {
      "tab_size": 4,
      "formatter": "language_server"
    },
    "Vue.js": {
      "formatter": {
        "external": {
          "command": "cat << EOF",
          "arguments": []
        }
      },
      "code_actions_on_format": {
        "source.fixAll.eslint": true
      }
    },
    "TypeScript": {
      "formatter": {
        "external": {
          "command": "cat << EOF",
          "arguments": []
        }
      },
      "code_actions_on_format": {
        "source.fixAll.eslint": true
      }
    }
  }
}
